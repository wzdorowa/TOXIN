mixin diagram(number, label, impressions)
  .diagram
    .diagram__image
      svg(width="100%" height="100%" viewBox="0 0 33.8 33.8" class="donut")
        block
      .diagram__text
        p.diagram__number= number
        p.diagram__label= label
    .diagram__list
      each val, index in impressions
        li.diagram__item(class="diagram__item_" + impressions[index].modifier)= impressions[index].text
